{
  "name": "parse-server",
  "version": "1.4.0",
  "description": "An example Parse API server using the parse-server module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParsePlatform/parse-server-example"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f envs/.env.production node index.js",
    "dev": "env-cmd -f envs/.env.development nodemon index.js",
    "docker:db": "docker-compose build && docker-compose up -d",
    "start:mhg": "env-cmd -f envs/.env.mhg node index.js",
    "dev:mhg": "env-cmd -f envs/.env.mhg nodemon index.js",
    "docker:db-mhg": "docker-compose --env-file envs/.env.mhg build && docker-compose --env-file envs/.env.mhg up -d",
    "start:amataya": "env-cmd -f envs/.env.amataya node index.js",
    "dev:amataya": "env-cmd -f envs/.env.amataya nodemon index.js",
    "docker:db-amataya": "docker-compose --env-file envs/.env.amataya build && docker-compose --env-file envs/.env.amataya up -d"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "express": "4.18.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.1",
    "parse": "^4.0.1",
    "parse-dashboard": "^5.0.0",
    "parse-server": "5.2.1",
    "pdf-creator-node": "^2.3.5",
    "phantomjs-prebuilt": "^2.1.16"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.17.0",
    "eslint": "8.15.0",
    "jasmine": "4.1.0",
    "mongodb-runner": "4.9.0",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "prettier": "2.6.2"
  },
  "engines": {
    "node": ">=12.22.10 <19"
  }
}
